{
    "version": 2,
    "build": [
        {
            "src": "notes_/wsgi.py",
            "use":"@vercel/python",
            "config":{ "maxLambdaSize":"15mb","runtime":"ptyhon3.9" }
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config": {
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"poll/wsgi.py"
        }
    ]
}
